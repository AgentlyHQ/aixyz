{
  "name": "aixyz",
  "version": "0.0.0",
  "description": "AI agent framework for building autonomous agents with X402 and ERC-8004 support.",
  "keywords": [
    "ai",
    "agent",
    "a2a",
    "mcp",
    "x402",
    "erc-8004",
    "framework"
  ],
  "homepage": "https://ai-xyz.dev",
  "bugs": {
    "url": "https://github.com/AgentlyHQ/aixyz/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AgentlyHQ/aixyz.git"
  },
  "license": "MIT",
  "author": "AgentlyHQ",
  "type": "module",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "default": "./index.js"
    },
    "./config": {
      "types": "./config.d.ts",
      "default": "./config.js"
    },
    "./accepts": {
      "types": "./accepts.d.ts",
      "default": "./accepts.js"
    },
    "./server": {
      "types": "./server/index.d.ts",
      "default": "./server/index.js"
    },
    "./server/adapters/a2a": {
      "types": "./server/adapters/a2a.d.ts",
      "default": "./server/adapters/a2a.js"
    },
    "./server/adapters/mcp": {
      "types": "./server/adapters/mcp.d.ts",
      "default": "./server/adapters/mcp.js"
    }
  },
  "bin": "bin.js",
  "files": [
    "**/*.d.ts",
    "**/*.d.ts.map",
    "**/*.js"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "tsc --build --clean"
  },
  "dependencies": {
    "@a2a-js/sdk": "^0.3.10",
    "@aixyz/config": "workspace:*",
    "@modelcontextprotocol/sdk": "^1.26.0",
    "@next/env": "^16.1.6",
    "@x402/core": "^2.3.0",
    "@x402/evm": "^2.3.1",
    "@x402/express": "^2.3.0",
    "@x402/mcp": "^2.3.0",
    "aixyz-cli": "workspace:*",
    "commander": "^13.0.0",
    "express": "^5.2.1",
    "jose": "^5.10.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "ai": "^6"
  },
  "peerDependencies": {
    "ai": "^6"
  },
  "peerDependenciesMeta": {
    "ai": {
      "optional": true
    }
  },
  "engines": {
    "bun": ">=1.3.0"
  }
}
